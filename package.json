{
  "name": "@regions-of-indonesia/solid-query",
  "version": "2.0.0",
  "description": "Regions of Indonesia Solid Query",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.modern.mjs",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "test-ui": "vitest --ui",
    "build": "microbundle src/index.ts --format modern,cjs --sourcemap false",
    "prerelease": "yarn run test && yarn run build",
    "release": "release-it --only-version"
  },
  "repository": "https://github.com/regions-of-indonesia/solid-query.git",
  "author": "flamrdevs <flamrdevs@gmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "dependencies": {
    "@regions-of-indonesia/client": "^3.0.0",
    "@tanstack/solid-query": "^5.0.0-alpha.7",
    "solid-js": "^1.6.15"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.7.0",
    "@vitest/ui": "^0.29.7",
    "javascript-yesterday": "^1.2.0",
    "jsdom": "^21.1.1",
    "microbundle": "^0.15.1",
    "node-fetch": "^3.3.1",
    "release-it": "^15.9.0",
    "vite-plugin-solid": "^2.6.1",
    "vitest": "^0.29.7"
  }
