{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
